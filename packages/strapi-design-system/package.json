    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@juggle/resize-observer": "^3.4.0",
    "@playwright/test": "1.27.1",
    "@strapi/icons": "^1.5.0",
    "@swc/core": "^1.3.11",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.4",
    "@types/jest": "^29.2.6",
    "@types/react": "17.0.2",
