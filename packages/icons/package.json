{
  "name": "@strapi/icons",
  "version": "2.1.2",
  "type": "module",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./symbols": {
      "types": "./dist/symbols-index.d.ts",
      "source": "./src/symbols-index.ts",
      "import": "./dist/symbols-index.mjs",
      "require": "./dist/symbols-index.js",
      "default": "./dist/symbols-index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@svgr/cli": "8.1.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "5.0.7",
    "styled-components": "6.1.11",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-externalize-deps": "^0.8.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "styled-components": "^6.0.0"
  },
  "scripts": {
    "build": "yarn clean:generated && yarn generate:icons && yarn generate:symbols && yarn build:prod",
    "build:prod": "vite build",
    "clean": "rimraf dist node_modules",
    "clean:generated": "rimraf src/icons src/symbols",
    "generate:icons": "svgr --config-file svgr.icons.config.cjs -- ./assets/icons",
    "generate:symbols": "svgr --config-file svgr.symbols.config.cjs -- ./assets/symbols"
  },
  "gitHead": "c74900b0ee3525510d266dc83c9743cb24dafced"
}
