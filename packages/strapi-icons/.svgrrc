# https://react-svgr.com/docs/options/

dimensions: true # keep viewBox
icon: '1.6rem'
svgProps: { 'fill': 'currentColor' }
jsxRuntime: 'automatic'
outDir: './src'
ref: true
typescript: true
svgoConfig:
  { plugins: [{ name: 'removeUselessStrokeAndFill', params: { stroke: true, fill: true, removeNone: false } }] }
