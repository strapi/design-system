      "types": "./dist/symbols.d.ts",
      "source": "./src/symbols.ts",
      "import": "./dist/symbols.mjs",
      "require": "./dist/symbols.js",
      "default": "./dist/symbols.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@strapi/pack-up": "^5.0.0",
    "@svgr/cli": "8.1.0",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "scripts": {
    "build": "yarn generate:icons && yarn build:prod",
    "build:prod": "pack-up build",
    "clean": "rimraf dist node_modules",
    "generate:icons": "svgr -- ./assets"
