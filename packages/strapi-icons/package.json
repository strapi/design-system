{
  "name": "@strapi/icons",
  "version": "2.0.0-beta.0",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
