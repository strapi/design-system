{
  "name": "@strapi/icons",
  "version": "1.4.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./dist/*"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/react": "17",
    "@types/react-dom": "17.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "clean-build": "rimraf dist src",
    "build": "yarn clean-build && yarn generate:icons && vite build && yarn generate:types",
    "generate:icons": "svgr --typescript --icon -d ./src ./assets/icons",
    "generate:types": "tsc --noEmit false --emitDeclarationOnly --declarationDir dist"
  },
  "gitHead": "bf52e690f743703a47f768ba83ebff55a3a12795"
}
