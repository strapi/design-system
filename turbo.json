{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "storybook-static/**"]
    },
    "@strapi/icons#build": {
      "dependsOn": ["^build"],
      "inputs": ["assets/**", ".svgrrc", "src/index.ts"],
      "outputs": ["dist/**", "src/**"]
    },
    "clean": {
      "cache": false,
      "persistent": true
    },
    "develop": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "format": {},
    "lint": { "dependsOn": ["^build"] },
    "test:unit": { "dependsOn": ["^build"] },
    "test:ts": { "dependsOn": ["^build"] }
  }
}
